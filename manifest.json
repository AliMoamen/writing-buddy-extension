{
  "manifest_version": 3,
  "name": "Writing Buddy Extension",
  "version": "1.0",
  "description": "Adds Extra Functionalities to Help Evaluate Writing.",
  "permissions": ["storage", "activeTab", "scripting"],
  "host_permissions": [
    "https://www.minerva.edu/admin/written_interview_decision/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.minerva.edu/admin/written_interview_decision/*"],
      "js": ["content.js"]
    }
  ]
}
